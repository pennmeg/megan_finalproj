<section id="countryMap" class="slides">
  <div class="wrapper">
    <div class="mapWrapper">
      <h3>Country Profiles: Women in STEM</h3>
      <div id="world-map" style="width: 600px; height: 400px"></div>
    </div>
    <div id="countryInfo">
      <% @countries.each do |c| %>
        <h3 id="countryName"><%= c[:name] %></h3>
      <% end %>
    </div>
  </div>
</section>
